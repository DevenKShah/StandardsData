    },
    {
      "skillId": "c24d1421-282c-4e18-91e3-c57d51025b92",
      "detail": "Communicating with colleagues or customers (internal or external)."
    },
    {
      "skillId": "8ecf75dd-fedc-4edc-aa99-8702209a1bd1",
      "detail": "Completing component or assembly documentation for example job instructions, drawings, quality control documentation."
    },
    {
      "skillId": "93e6648e-6986-46eb-bca1-dababac16e4e",
